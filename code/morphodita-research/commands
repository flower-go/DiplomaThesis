qsub -cwd -o outputs/$(date +%m%d-%H%M)_baseline_o -e outputs/$(date +%m%d-%H%M)_baseline_e -q gpu.q -l gpu=1,mem_free=16G,h_data=16G -pe smp 2 $HOME/env_diplomka/bin/python3 morpho_tagger_2.py ~doubrap1/pdt/pdt-3.5 --embeddings ~doubrap1/embeddings/cs_lindat4gw/forms.vectors-w5-d300-ns5.npz --threads 2 --debug_mode 1 --checkp "ch0"
grep '^Test' outputs/$fi | awk -F'[ ,]' 'BEGIN{OFS="\t";} {print "mBERT","base","base","5e-5","64","all",$4, $13, $16, $19, $22;}' >> final_results
qsub -cwd -o outputs/$(date +%m%d-%H%M)_base_ls_o -e outputs/$(date +%m%d-%H%M)_base_ls_e -q gpu.q -l gpu=1,mem_free=16G,h_data=16G -pe smp 2 $HOME/env_diplomka/bin/python3 morpho_tagger_2.py ~doubrap1/pdt/pdt-3.5 --embeddings ~doubrap1/embeddings/cs_lindat4gw/forms.vectors-w5-d300-ns5.npz --threads 2 --debug_mode 1 --checkp "ch1" --label_smoothing 0.03
grep '^Test' outputs/$fi | awk -F'[ ,]' 'BEGIN{OFS="\t";} {print "mBERT","ls","base","5e-5","64","all",$4, $13, $16, $19, $22;}' >> final_results
qsub -cwd -o outputs/$(date +%m%d-%H%M)_embed_o -e outputs/$(date +%m%d-%H%M)_embed_e -q gpu.q -l gpu=1,mem_free=16G,h_data=16G -pe smp 2 $HOME/env_diplomka/bin/python3 morpho_tagger_2.py ~doubrap1/pdt/pdt-3.5 --embeddings ~doubrap1/embeddings/cs_lindat4gw/forms.vectors-w5-d300-ns5.npz --threads 2 --debug_mode 1 --checkp "ch2" --label_smoothing 0.03 --bert ""bert-base-multilingual-uncased""
grep '^Test' outputs/$fi | awk -F'[ ,]' 'BEGIN{OFS="\t";} {print "mBERT","embed","base","5e-5","64","all",$4, $13, $16, $19, $22;}' >> final_results
qsub -cwd -o outputs/$(date +%m%d-%H%M)_bertfine_o -e outputs/$(date +%m%d-%H%M)_bertfine_e -q gpu.q -l gpu=1,mem_free=16G,h_data=16G -pe smp 2 $HOME/env_diplomka/bin/python3 morpho_tagger_2.py ~doubrap1/pdt/pdt-3.5 --embeddings ~doubrap1/embeddings/cs_lindat4gw/forms.vectors-w5-d300-ns5.npz --threads 2 --debug_mode 1 --checkp "ch3" --label_smoothing 0.03 --bert_model "bert-base-multilingual-uncased" --cont 1 --epochs "4:5e-5" --accu 16 --batch_size 4 --test_only "checkpoints/ch2"
grep '^Test' outputs/$fi | awk -F'[ ,]' 'BEGIN{OFS="\t";} {print "mBERT","fine","base","5e-5","4","all",$4, $13, $16, $19, $22;}' >> final_results
